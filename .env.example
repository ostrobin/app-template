# MyApp Configuration
# Copy relevant vars to /srv/.env with MYAPP_ prefix

# Database (via pgbouncer)
MYAPP_DATABASE_URL=postgresql+asyncpg://myapp:CHANGEME@pgbouncer:5432/myapp

# Logging
MYAPP_LOG_LEVEL=info

# Authentik SSO (optional)
MYAPP_AUTHENTIK_CLIENT_ID=myapp
MYAPP_AUTHENTIK_CLIENT_SECRET=CHANGEME_FROM_AUTHENTIK
